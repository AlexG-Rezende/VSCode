/* Hotéis - Página inicial */
.hotel-containers {
  display: flex; /* Usando Flexbox para alinhar os cards */
  flex-wrap: wrap; /* Permite quebra de linha quando necessário */
  justify-content: center; /* Espaçamento uniforme entre os cards */
  gap: 30px; /* Espaçamento entre os cards */
  padding: 30px; /* Espaçamento interno */
  background-color: #eeeeee; /* Cor de fundo suave */
}

/* Estilo individual dos cards */
.hotel-container {
  background-color: #fff; /* Fundo branco para os cards */
  border-radius: 7px; /* Cantos arredondados */
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1); /* Sombra para destacar os cards */
  overflow: hidden; /* Garante que o conteúdo respeite os limites do card */
  max-width: 300px; /* Largura máxima de cada card */
  text-align: center; /* Centraliza os textos no card */
  transition: transform 0.3s ease, box-shadow 0.3s ease; /* Efeito suave ao passar o mouse */
}

/* Efeito hover para o card */
.hotel-container:hover {
  transform: translateY(-5px); /* Move o card levemente para cima */
  box-shadow: 0 8px 12px rgba(0, 0, 0, 0.2); /* Intensifica a sombra */
}

/* Estilo da imagem */
.hotel-container img {
  width: 100%; /* Imagem ocupa toda a largura do card */
  height: auto; /* Ajusta a altura proporcionalmente */
  border-bottom: 3px solid #ccc; /* Linha para separar imagem e conteúdo */
}

/* Título do card */
.hotel-container h3 {
  font-size: 1.2rem; /* Tamanho do título */
  color: #333; /* Cor do texto */
  margin: 10px 0; /* Espaçamento acima e abaixo */
  font-weight: bold; /* Destaca o texto */
}

/* Parágrafo dentro do card */
.hotel-container p {
  font-size: 1rem; /* Tamanho do texto */
  color: #000000; /* Cor do texto */
  line-height: 1.5; /* Espaçamento entre linhas */
  margin: 0 10px 15px; /* Espaçamento interno */
}

/* Botão dentro do card */
.hotel-container a {
  display: inline-block; /* Define o botão como bloco inline */
  text-decoration: none; /* Remove o sublinhado */
  color: white; /* Cor do texto */
  background-color: #007BFF; /* Cor do botão */
  padding: 10px 15px; /* Espaçamento interno */
  border-radius: 5px; /* Cantos arredondados do botão */
  font-size: 1rem; /* Tamanho do texto */
  font-weight: bold; /* Destaca o texto */
  transition: background-color 0.3s ease; /* Efeito de transição suave */
}

/* Efeito hover para o botão */
.hotel-container a:hover {
  background-color: #0056b3; /* Escurece a cor do botão ao passar o mouse */
}

/* Estilo para os links que envolvem os cards */
.hotel-card {
  text-decoration: none; /* Remove o sublinhado do link */
  color: inherit; /* Mantém a cor do texto do card */
  display: block; /* Faz o link ocupar todo o card */
}

.footer {
  bottom: 0;
  width: 100%;
  background: #013668;
  color: white;
  text-align: center;
  padding: 10px;
}

