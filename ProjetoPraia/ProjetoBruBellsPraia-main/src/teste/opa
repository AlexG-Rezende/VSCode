  width: 173px;
  height: 73px;
}

.nav-links {
  list-style: none;
  display: flex;
  gap: 60px;
  letter-spacing: 2px;
}

.nav-links li a {
  text-decoration: none;
  color: white;
  font-size: 21px;
  transition: color 0.3s ease;
}

.nav-links li a:hover {
  color: #ffae00;
}

/* Carousel */
.carousel {
  margin-top: -94px; /* Ajuste para não sobrepor a navbar */
}

.carousel-inner img {
  width: 80%; /* Controle da largura da imagem */
  margin: 0 auto; /* Centraliza as imagens no carrossel */
  object-fit: cover; /* Mantém a proporção */
}

.carousel-descricao {
  position: absolute;
  bottom: 10%; /* Ajuste a posição vertical */
  left: 50%;
  transform: translateX(-50%);
  background-color: rgba(1, 54, 104, 0.85);
  color: white; /* Cor do texto */
  padding: 15px 20px; /* Espaçamento interno */
  border-radius: 7px; /* Bordas arredondadas */
  max-width: 60%; /* Limita a largura do contêiner */
  text-align: center; /* Centraliza o texto */
  font-family: 'Arial', sans-serif; /* Fonte padrão */
  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.3); /* Sombra suave */
  z-index: 10; /* Garante que o texto esteja acima de outros elementos */
}

.carousel-descricao h5 {
  font-size: 1.5rem; /* Tamanho do título */
  margin-bottom: 10px; /* Espaçamento abaixo do título */
  font-weight: bold; /* Negrito para o título */
}

.carousel-descricao p {
  font-size: 1rem; /* Tamanho do parágrafo */
  margin: 0; /* Remove margens */
  line-height: 1.5; /* Espaçamento entre linhas */
}

/* Section Containers */
